<h1>Create new organization</h1>
<%= form_for @organization, url: {action: 'create'} do |f| %>
    <div class="form-group">
        <%= f.label :name %><br/>
        <%= f.text_field :name, :class => 'form-control' %>
    </div>
    <div class="form-group">
        <%= f.submit "create", :class => 'btn btn-primary' %>
    </div>
<% end %>